CREATE TABLE landslides(
  "id" INTEGER,
  "date" TEXT,
  "time" TEXT,
  "country" TEXT,
  "nearest_places" TEXT,
  "hazard_type" TEXT,
  "landslide_type" TEXT,
  "trigger" TEXT,
  "storm_name" TEXT,
  "fatalities" INTEGER,
  "injuries" TEXT,
  "source_name" TEXT,
  "source_link" TEXT,
  "location_description" TEXT,
  "location_accuracy" TEXT,
  "landslide_size" TEXT,
  "photos_link" TEXT,
  "cat_src" TEXT,
  "cat_id" INTEGER,
  "countryname" TEXT,
  "near" TEXT,
  "distance" TEXT,
  "adminname1" TEXT,
  "adminname2" TEXT,
  "population" INTEGER,
  "countrycode" TEXT,
  "continentcode" TEXT,
  "key" TEXT,
  "version" TEXT,
  "tstamp" TEXT,
  "changeset_id" INTEGER,
  "latitude" REAL,
  "longitude" REAL,
  "geolocation" TEXT
);
